<header>
    <div id="logo">
      <a href={{ path_for("root") }}>
          <img src="/images/rpgcLogotype.png" height="60px" alt="dice" />
      </a>
    </div>

    <nav>		
        <ul>
    		<li><a href={{ path_for("root") }}>Home</a></li>
      		<li><a href={{ path_for("about") }}>About</a></li>
      		{% if currentUser %}
      		    <li><a href={{ path_for("user") }}>Manage Account</a></li>
      		    <li><a href={{ path_for("rooms") }}>Manage Rooms</a></li>
      		{% else %}
      		    <li><a href={{ path_for("register") }}>Register</a></li>
      		{% endif %}
      		<li><a href={{ path_for("contact") }}>Contact</a></li>
     		<li><a href={{ path_for("help") }}>Help</a></li>
    	</ul>
    </nav>

	<div id="logindiv">
        {% if currentUser %}
	        <button class="loginbtn">
	            <a href={{ path_for("logout") }}>Logout</a>
	        </button>
	    {% else %}
	        <button class="loginbtn">Login</button>
	    {% endif %}
	</div>
</header>